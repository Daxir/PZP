<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>


<?import javafx.scene.control.PasswordField?>
<AnchorPane prefHeight="600.0" prefWidth="799.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FirstLaunchController">
   <children>
      <Label layoutX="341.0" layoutY="202.0" text="Create your password:" />
      <PasswordField fx:id="passwordText" layoutX="325.0" layoutY="231.0" promptText="Password..." />
      <PasswordField fx:id="passwordRepeatText" layoutX="325.0" layoutY="266.0" promptText="Repeat Password..." />
      <Button fx:id="commitButton" layoutX="370.0" layoutY="300.0" mnemonicParsing="false" onAction="#setPassword" text="Commit" />
   </children>
</AnchorPane>
